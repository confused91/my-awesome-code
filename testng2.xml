<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="parallel suite" verbose="1">
    <test name="browser tests on chrome" >
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="selenium.page_object_model.FacebookLoginTest"></class>
            <class name="selenium.page_object_model.CreateAccountTest"></class>
        </classes>
    </test>
    <test name="browser tests on firefox" >
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="selenium.page_object_model.FacebookLoginTest"></class>
        </classes>
    </test>
</suite>
